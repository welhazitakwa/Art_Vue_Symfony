				<script>
					// Créer un tableau de valeurs de prix à partir des données récupérées
var values = [];
var labels = [];

// Récupérer les prix min et max
var minPrice = Number.MAX_SAFE_INTEGER;
var maxPrice = Number.MIN_SAFE_INTEGER;{% for data in prixVenteData %}
var price = {{ data.price }};
minPrice = Math.min(minPrice, price);
maxPrice = Math.max(maxPrice, price);{% endfor %}

// Déterminer le nombre d'intervalles
var numIntervals = 5;
var intervalSize = (maxPrice - minPrice) / numIntervals;

// Initialiser les compteurs d'intervalles
var intervalCounters = new Array(numIntervals).fill(0);

// Compter le nombre d'éléments dans chaque intervalle
{% for data in prixVenteData %}
var price = {{ data.price }};
var intervalIndex = Math.floor((price - minPrice) / intervalSize);
intervalCounters[intervalIndex]++;{% endfor %}

// Générer les étiquettes pour chaque intervalle
for (var i = 0; i < numIntervals; i++) {
var startInterval = minPrice + (i * intervalSize);
var endInterval = minPrice + ((i + 1) * intervalSize);
labels.push('De ' + startInterval.toFixed(2) + ' à ' + endInterval.toFixed(2));
values.push(intervalCounters[i]);
}

// Données du graphique en barres
var data = {
labels: labels,
datasets: [
{
label: 'Nombre d\'œuvres d\'art',
data: values,
backgroundColor: 'rgba(54, 162, 235, 0.2)',
borderColor: 'rgba(54, 162, 235, 1)',
borderWidth: 1
}
]
};

// Options du graphique en barres
var options = {
title: {
display: true,
text: 'Répartition des prix de vente par intervalle'
},
scales: {
yAxes: [
{
ticks: {
beginAtZero: true
}
}
]
}
};

// Création du graphique en barres
var ctx = document.getElementById('bar-chart').getContext('2d');
var pieChart = new Chart(ctx, {
type: 'bar',
data: data,
options: options
});
				</script>

