{% extends 'base.html.twig' %}

{% block title %}Vote index{% endblock %}

{% block body %}
    <h1>Vote index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Note</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for vote in votes %}
            <tr>
                <td>{{ vote.id }}</td>
                <td>{{ vote.note }}</td>
                <td> 
                 {{ include('vote/_delete_form.html.twig') }}
                  </td>
               
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
                  
            </tr>
        {% endfor %}
        </tbody>
    </table>

    
{% endblock %}
